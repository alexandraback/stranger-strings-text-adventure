{
  "name": "jz-text-adventure",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "gh-pages": "^1.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.17.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --exclude **/*.d.ts src/**/*.ts src/**/*.tsx --fix",
    "prettier": "prettier --write --single-quote --trailing-comma es5 --tab-width 4 src/**/*.js src/**/*.ts src/**/*.tsx src/**/*.css"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.8",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "prettier": "^1.14.0",
    "typescript": "^2.8.3"
  }
}
