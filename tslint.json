{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-console": [false, "log", "error"],
    "max-classes-per-file": [false]
  }

}
